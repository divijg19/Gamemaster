{
  "db_name": "PostgreSQL",
  "query": "SELECT u.unit_id, u.name, u.description, u.base_attack, u.base_defense, u.base_health, u.is_recruitable, u.kind as \"kind: UnitKind\", u.rarity as \"rarity: UnitRarity\",\n        COALESCE(he.defeats,0) as defeats, he.last_defeated_at,\n        (SELECT 1 FROM drafted_human_contracts d2 WHERE d2.user_id=$1 AND d2.unit_id=u.unit_id AND d2.consumed=FALSE) as drafted_active,\n        (SELECT 1 FROM player_units pu WHERE pu.user_id=$1 AND pu.unit_id=u.unit_id LIMIT 1) as recruited\n        FROM units u LEFT JOIN human_encounters he ON he.user_id=$1 AND he.unit_id=u.unit_id WHERE u.kind='Human'",
